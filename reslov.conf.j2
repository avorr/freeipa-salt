# Generated by dhcpcd from eth0.dhcp
# /etc/resolv.conf.head can replace this line
domain {{ pillar['freeipa-client']['ipa_dns_zone'] }}
{% for ipa_main in (pillar['freeipa-client']['ipa_main_servers_ip'] + (pillar['freeipa-client']['ipa_servers_ip'] or []))[:3] -%}
nameserver {{ ipa_main }}
{% endfor -%}
options timeout:1
